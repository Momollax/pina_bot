import requests
from bs4 import BeautifulSoup
from urllib.parse import quote
import re

user_agent = 'Google Chrome on Windows 10 - "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/89.0.4389.82 Safari/537.36"'
headers = {
    'User-Agent': user_agent
}

# Ouvrir le fichier en mode lecture
with open('common.txt', 'r') as fichier:
    lignes = fichier.readlines()

base_url = "https://5e.tools/js/items.js"
response = requests.get(base_url, headers=headers)

soup = BeautifulSoup(response.content, 'html.parser')

# Écrire le contenu de soup dans le fichier toto.txt
with open('toto.txt', 'w') as toto_fichier:
    toto_fichier.write(str(soup))

print("Contenu de soup écrit dans toto.txt")
